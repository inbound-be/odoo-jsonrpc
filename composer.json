{
    "name": "inbound-be/odoo-jsonrpc",
    "description": "PHP Odoo Json-RPC connector in PHP 7.4",
    "keywords": [
        "php",
        "odoo",
        "jsonrpc",
        "laravel"
    ],
    "homepage": "https://github.com/inbound-be/odoo-jsonrpc",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Oliver Buchmann",
            "email": "o.buchmann@pc-web.at"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/inbound-be/odoo-jsonrpc.git"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^6.5",
        "doctrine/annotations": "^1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Obuchmann\\OdooJsonRpc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Obuchmann\\OdooJsonRpc\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Obuchmann\\OdooJsonRpc\\OdooServiceProvider"
            ]
        }
    }

}
